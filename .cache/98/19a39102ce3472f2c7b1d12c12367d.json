 vertical-align: middle;\n    }\n}\n\n\n@import 'logo', 'navigation';\n","../src/scss/_animations.scss":"\n@keyframes reveal-from-left {\n    0% {\n        clip-path: inset(0);\n    }\n    100% {\n        clip-path: inset(0 0 0 100%);\n    }\n}\n\n@keyframes hide-line {\n    0% {\n        clip-path: inset(0 100% 0 0);\n        z-index: 50;\n    }\n    100% {\n        clip-path: inset(0);\n        z-index: 50;\n    }\n}\n\n@keyframes list-item-line {\n    0% {\n        height: 0vh;\n    }\n    50% {\n        height: 34vh;\n        bottom: -33vh;\n    }\n    100% {\n        height: 0vh;\n        bottom: 1vh;\n    }\n}","../src/scss/_logo.scss":"@import 'animations', 'mixins';\n\n$logo-width: 300px;\n\n.logo {\n    font-size: 1.5rem;\n    line-height: 3.8rem;\n    position: relative;\n    min-width: $logo-width;\n    margin-bottom: 100px;\n    font-family: 'Kumar One', cursive;\n    &__text {\n        background-color: $main-grey;\n        position: relative;\n        z-index: 100;\n        text-align: center;\n    }\n    &__underline {\n        @include underline(3px, 22px);\n    }\n    &__clipcover {\n        @include clipcover-animation(.5s, .5s, 1s, 1.5s); \n    }\n    \n}","../src/scss/_mixins.scss":"@import 'animations';\n\n@mixin underline ($height, $top-offset){\n    z-index: 0;\n    position: absolute;\n    left: calc(-50vw + 50%);\n    min-height: $height;\n    top: calc(50% - #{$top-offset});\n    width: 100vw;\n    background: linear-gradient($gradient-direction, $start-gradient, $end-gradient);\n}\n\n@mixin clipcover-animation($reveal-duration, $hide-duration, $reveal-delay, $hide-delay) {\n    position: absolute;\n    width: 100vw;\n    left: calc(-50vw + 50%);\n    top: -20px;\n    height: 140px;\n    z-index: 150;\n    background-color: $main-grey;\n    animation-name: reveal-from-left, hide-line;\n    animation-duration: $reveal-duration, $hide-duration;\n    animation-delay: $reveal-delay, $hide-delay;\n    animation-iteration-count: 1, 1;\n    animation-fill-mode: none, forwards;    \n}","../src/scss/_navigation.scss":"@import 'animations', 'mixins';\n\n$list-width: 600px;\n\n.nav-container {\n    position: relative;\n}\n\n.nav-list {\n    font-size: 1.5rem;\n    display: flex;\n    width: $list-width;\n    justify-content: space-between;\n    align-items: center;\n    &__item {\n        cursor: pointer;\n        background-color: $main-grey;\n        position: relative;\n        z-index: 100;\n        text-align: center;\n        &::after {\n            content: \"\";\n            width: 2px;\n            height: 0px;\n            position: absolute;\n            background-color: tomato;\n            left: 15%;\n            bottom: -33vh;\n            animation-duration: .5s;\n        }\n        &:hover::after {\n            animation-name: list-item-line;\n        }\n    }\n}\n\n.nav-list__underline {\n    @include underline(2px, -5px)\n}\n\n.nav-list__clipcover {\n    @include clipcover-animation(.5s, .5s, 2.5s, 3s);   \n}\n\n"},"lineCount":null}},"error":null,"hash":"9bc8972588a0169ffa30090f4151baa0","cacheData":{"env":{}}}